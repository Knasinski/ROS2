    "version": "0.2.0",
    "configurations": [
        { 
            "name": "(gdb) Attach",
            "type": "cppdbg",
            "request": "attach",
            "program": "/home/al/catkin_ws/devel/lib/beginner_tutorials/add_two_ints_client",
            "processId": "${command:pickProcess}",
            "MIMode": "gdb"
        },
        {
            "name": "(gdb) Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "/home/al/catkin_ws/devel/lib/beginner_tutorials/add_two_ints_client",
            "args": ["1","4"],
            "stopAtEntry": false,
            "cwd": "/home/al/catkin_ws/devel/lib/beginner_tutorials",
            "environment": [],
            "externalConsole": true,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}